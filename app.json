{
    "name": "VideoStream",
    "description": ".هنا حط اسم لبوت يفضل بالانجليزية ",
    "keywords": ["bot", "telegram"],
    "repository": "https://github.com/galihmrd/Group-calls-video",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "هنا حط كود بايروكرام سيشن (P)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "حط هنا توكن البوت تحصلة من @BotFather",
            "required": true
        },
        "API_ID": {
            "description": "الايـبـي أيـدي من my.telegram.org",
            "required": true
        },
        "API_HASH": {
            "description": "ايــبـي هــاش my.telegram.org",
            "required": true
        },
        "SUDO_USERS": {
            "description": "حط هنا ايديك",
            "required": true
        },
        "BOTLOG_CHATID": {
            "description": "حط هنا معرف قناتك (صعد البوت ادمن) ",
            "required": true
        },
        "REST_API": {
            "description": "عوفهة",
            "required": false
        }
    },
    "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
    }
  ],
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ]
}
